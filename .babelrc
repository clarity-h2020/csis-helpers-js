{
  "exclude": [
    "node_modules/**",
    "*.json"
  ],
  "presets": [
    [
      "@babel/preset-env",
      {
        "modules": false
      }
    ],
    
    "@babel/preset-react"
  ],
  "plugins": [
    ["@babel/transform-runtime"],
    [
      "@babel/plugin-proposal-class-properties"
    ],
    [
      "@babel/plugin-proposal-export-default-from"
    ]
  ]
}